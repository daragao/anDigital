{
    "parser": "babel-eslint",
    "rules": {
        "strict":0,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
    },
    "env": {
        "es6": true,
        "browser": true,
        "amd": true,
        "jquery": false
    },
    "globals": {
        "$":true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}
